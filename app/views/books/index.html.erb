<h1>Books</h1>
<table>
  <tr>
    <thead>
      <th>Title</th>
      <th>Body</th>
      <th> </th>
    </thead>
    <tbody>

    </tbody>
  </tr>
</table>

<h2>New book</h2>
<%= form_with model: @books do |f| %>
  <h4>Title</h4>
  <%= f.text_field :title %>

  <h4>Body</h4>
  <%= f.text_area :body %>

  <%= f.submit 'Create Books' %>
<% end %>